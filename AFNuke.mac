sub main
/declare dummy int local
/call GeneralDeclares	
/call GeneralDeclares2
/call AAInit
/call SetupAdvLootVars
/call event_zone
/echo SpellSet: ${SpellSet}
/squelch /alias /fire /echo SpellSet: Fire
/squelch /alias /ice /echo SpellSet: Ice
/squelch /alias /magic /echo SpellSet: Magic
/echo  =+= Azum's AutoNuke Macro Enabled =+=
/echo Your Main assist is ${MainAssist}
/echo Your Secondary assist is ${SecondAssist}
/echo Your Third assist is ${TripleAssist}
 /if (${UseIRC}) /call Irc 
 
| ---------------------------------
:mainloop
/varset MacroState MainLoop
/if (${Died} || ${Me.Dead}) /call Wait4Rez
/doevents
/if (${Me.Pet.ID} && ${Me.Pet.Name.Find[familiar]}) /pet get lost
/if (${SpawnCount[npc radius ${EngageDistance}]}) /call DPS
/call MedTime
/if (${DoLoot} && !${Me.Moving} && !${Me.Invis}) /call LootMobs
/if (${DoLeash}) /call Leash
/if (!${bufftimer} || !${Me.CombatState.Equal[Combat]}) /call CheckBuffs
/if (!${CheckExpTimer}) /call AutoAdjustExp
/if (${UseIRC} && !${Irc} && !${irctimer}) /call Irc
/call SpecialSubs
/goto :mainloop
/endmacro
	
Sub DPS
/varset MacroState DPS
/doevents
/call GetTarget
/if (${validTarget} && ${Target.PctHPs}<=${mobhealth} && ${Target.Type.Equal[NPC]} && ${Target.Distance} < ${EngageDistance} && ${Target.ID} && ${Target.LineOfSight}) /goto :killtarget
/return
:killtarget
/Echo ${Target.CleanName} is at the distance of: ${Target.Distance}
:Combat
/if (${Died} || ${Me.Dead}) /return
/if (!${Spawn[${CurrentTarget}].ID} || ${Spawn[${CurrentTarget}].Type.Equal[corpse]} || !${Target.ID}) /varset validTarget 0
/doevents
/call SpecialSubs
/call SpecialNukes
/call Nuke
/if (${validTarget}) /goto :Combat
/return

Sub SpecialNukes
/if (${Died} || ${Me.Dead}) /return
/varset MacroState SpecialNukes
/if (${Target.Distance} < 70 ) /call castspell "banestrike" alt ${mobid} checkall
/if (!${LowerElementTimer${CurrentTarget}}) /call CastDebuff "Lower Element" alt ${CurrentTarget} LowerElementTimer${CurrentTarget} 5m
/if (${Me.TargetOfTarget.Name.Equal[${Me.Name}]} && ${NameList.Find[${Target.CleanName}]} && ${Me.AltAbilityReady[Mind Crash]}) /call castspell "Mind Crash" alt 0
/if (${Me.AltAbilityReady[Improved Twincast]} && ${Target.PctHPs}<${WhenToBurn} && ${NameList.Find[${Target.CleanName}]} && (!${Me.Buff[Twincast].ID} || !${Me.Buff[Twincast rk. ii].ID} || !${Me.Buff[Twincast rk. iii].ID})) {
	/call castspell "Improved Twincast" alt 0
} else /if (${Me.SpellReady[${TwinCast}]} && !${Me.Buff[Twincast].ID} && ${Target.PctHPs}<${WhenToBurn} && ${NameList.Find[${Target.CleanName}]} || !${Raid.Members} && ${Me.SpellReady[${TwinCast}]} && ${SpawnCount[npc radius 50 los]} > 4) {
	/call castSpell "${TwinCast}" gem1 0
}
/if (${Me.AltAbilityReady[Prolonged Destruction]} && ${Target.PctHPs}<${WhenToBurn} && ${NameList.Find[${Target.CleanName}]} && ${Me.Buff[Twincast].ID}) {
	/call CastSpell "Prolonged Destruction" alt 0 
	/call Burn
} else /if (${Me.AltAbilityReady[Arcane Destruction]} && ${Target.PctHPs}<${WhenToBurn} && ${NameList.Find[${Target.CleanName}]} && !${Me.Song[Prolonged Destruction].ID} && (${Me.Buff[Twincast].ID} || ${Me.Buff[Twincast rk. ii].ID} || ${Me.Buff[Twincast rk. iii].ID})) {	
	/call CastSpell "Arcane Destruction" alt 0 
	/call Burn
}
/if (${Me.AltAbilityReady[conflagration]}) /call cast "conflagration" alt 0
/if (${Me.AltAbilityReady[ice comet]}) /call cast "ice comet" alt 0
/if (${Target.Distance}<250 && !${Me.SpellReady[${SmallNuke}]}) /call CastSpell "Force of Will" alt ${mobid} checkall
/if (${Target.Distance}<250 && !${Me.SpellReady[${SmallNuke}]}) /call CastSpell "Force of Flame" alt ${mobid} checkall
/if (${Target.Distance}<250 && !${Me.SpellReady[${SmallNuke}]}) /call CastSpell "Force of Ice" alt ${mobid} checkall
/if (!${Snared} && ${Me.AltAbilityReady[Atol's Shackles]} && ${Target.Fleeing} && ${Target.PctHPs} < 30 && ${DoSnare}) /call NukeCast "Atol's Shackles" Alt "Snare"
/return

Sub Burn
/call CastSpell "${FundamentalName}" alt 0
/call CastItemClicky "staff of phenomenal power" 
/if (!${FindItem[${ClickyBP}].Timer}) /call CastItemClicky "${ClickyBP}" 
/call CastSpell "Fury of KeraFyrm" alt 0
/if (${Me.AltAbilityReady[Call of Xuzl]} && ${doxuzl}) /call CastSpell "Call Of Xuzl" alt 0
/if (${Me.AltAbilityReady[Acute Focus of arcanum]}) {
   	 /call CastSpell "Acute Focus Of Arcanum" alt 0
} else {
   	/if (${Me.AltAbilityReady[Focus of arcanum]}) /call CastSpell "Focus of Arcanum" alt 0
}
/call CastSpell "Arcane Whisper" alt 0
/call CastSpell "Silent Casting" alt 0
/if (${Me.AltAbilityReady[Glyph of Courage]} && ${Raid.Members}) /call CastSpell "Glyph of Courage" alt 0
/return

	  
Sub CheckAll
/varset MacroState CheckAll
/if (${Died} || ${Me.Dead}) /return
/doevents 
/declare i int local
/if (${Target.Type.Equal["Corpse"]} || !${Target.ID}) {
	/call interrupt
}
/if (${Zone.ShortName.Equal[discordtower_raid]} && ${Spawn[The Mindblight].ID} && ${Spawn[The Mindblight].Distance}<50) /call MoveAwayFromMindBlight
	 /if (${Zone.ShortName.Equal[crystallos_raid]} && ${Spawn[Tempest of Vyskudra].ID} && ${Spawn[Tempest of Vyskudra].Distance}>30) /call MoveToTornado
/if (${Target.ID}) {
   	/for i 1 to ${Target.BuffCount}
   /if (${MezSpellList.Find[${Target.Buff[${i}]}]}) {
   	/call interrupt
   	/clear Target
   	/return
  }
  /next i
}
/return
