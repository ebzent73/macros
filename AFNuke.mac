Sub Harvestcheck
/varset MacroState Harvestcheck
/call Rod
/if ((${Me.Buff[Twincast rk. iii].ID} || ${Me.Buff[Twincast rk. ii].ID} || ${Me.Buff[Twincast].ID} || ${Me.Song[${GiftOfMana}].ID} || ${Me.Song[Thricewoven Power].ID} )) /return
/if (${Me.PctMana}<=30 && !${Me.Moving} && ${Me.SpellReady["${HarvestSpell}"]} && !${Me.Casting.ID}) /call CastSpell "${HarvestSpell}" ${harvestgem} 0 	
/if (${Me.PctMana}<=${harvestpercent} && !${Me.Moving} && ${Me.AltAbilityReady["172"]} && !${Me.Casting.ID}) /call Cast "172" alt 7s
/return

Sub CheckBuffs
/varset MacroState CheckSelfBuff

/declare i int local
/declare Bard bool local FALSE
/if (${Zone.ID}!=${currentZone}) /call Event_Zone
/if ( !${NumSelfBuff} || ${Me.Invis} || ${Me.Dead} ) /return
/call CheckBuffies
/varset bufftimer 20s
/call ReturnCurrentGroup
/if (${Macro.Return.Find[brd]}) /varset Bard True
	 /if (!${Me.Buff[Kerafyrm's Prismatic familiar].ID} && ${Me.AltAbilityReady[Kerafyrm's prismatic familiar]}) {
	 	/if (${Me.Buff[${mainfamiliar}].ID}) /notify BuffWindow Buff${Math.Calc[${Me.Buff[${mainfamiliar}].ID}-1].Int} leftmouseup
	 	/if (${Me.Buff[Improved Familiar].ID}) /notify BuffWindow Buff${Math.Calc[${Me.Buff[Improved Familiar].ID}-1].Int} leftmouseup
	 	 /if (${Me.Buff[Familiar: Emerald Sokokar].ID}) /notify BuffWindow Buff${Math.Calc[${Me.Buff[Familiar: Emerald Sokokar].ID}-1].Int} leftmouseup
    /if (${Me.AltAbilityReady[Kerafyrm's prismatic familiar]}) /call cast "Kerafyrm's prismatic familiar" alt 2s
    /return
  }
   /if (${Bard} && !${Me.Buff[Improved Familiar].ID} && !${Me.Buff[Kerafyrm's Prismatic Familiar].ID}) {
    /if (${Me.Buff[${mainfamiliar}].ID}) /notify BuffWindow Buff${Math.Calc[${Me.Buff[${mainfamiliar}].ID}-1].Int} leftmouseup
      /if (${Me.Buff[Familiar: Emerald Sokokar].ID}) /notify BuffWindow Buff${Math.Calc[${Me.Buff[Familiar: Emerald Sokokar].ID}-1].Int} leftmouseup
    /if (${Me.AltAbilityReady[Improved Familiar]}) /call cast "Improved Familiar" alt 2s
   }
	/if (!${Me.Buff[${mainfamiliar}].ID} && !${Bard} && !${Me.Buff[Kerafyrm's Prismatic Familiar].ID}) {
	/if (${Me.Buff[Improved Familiar].ID}) /notify BuffWindow Buff${Math.Calc[${Me.Buff[Improved Familiar].ID}-1].Int} leftmouseup
   /if (${Me.Buff[${firefamiliar}].ID}) /notify BuffWindow Buff${Math.Calc[${Me.Buff[${firefamiliar}].ID}-1].Int} leftmouseup
   /if (${Me.Buff[${icefamiliar}].ID}) /notify BuffWindow Buff${Math.Calc[${Me.Buff[${icefamiliar}].ID}-1].Int} leftmouseup
   /if (${Me.Buff[${magicfamiliar}].ID}) /notify BuffWindow Buff${Math.Calc[${Me.Buff[${magicfamiliar}].ID}-1].Int} leftmouseup
	/if (${Me.AltAbilityReady[${mainfamiliar}]}) /call cast "${mainfamiliar}" alt 2s		
	}
/if (!${Me.Buff[Mana Reverberation].ID} && !${Me.Buff[PyroMancy].ID} && !${Me.Song[Mana Reverberation].ID} && !${Me.Buff[Mana Reverberation rk. ii].ID} && !${Me.Buff[Mana Reverberation rk. iii].ID} && !${MeSong[WildSpark Aura Effect iii].ID}) /call castspell "Pyromancy" alt 0 
/if (${Me.Height}>2.03 && ${FindItem[${ItemShrink}].ID}) /call CastSpell "${ItemShrink}" item ${Me.ID}
/return


	
